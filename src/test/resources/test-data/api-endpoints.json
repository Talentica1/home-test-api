{
  "endpoints": {
    "inventory": {
      "getAll": "/api/inventory",
      "filter": "/api/inventory/filter",
      "add": "/api/inventory/add"
    }
  },
  "statusCodes": {
    "success": 200,
    "badRequest": 400,
    "unauthorized": 401,
    "notFound": 404,
    "serverError": 500
  },
  "headers": {
    "contentType": "application/json",
    "accept": "application/json"
  },
  "timeouts": {
    "connection": 30000,
    "read": 30000
  }
} 